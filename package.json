    "lint": "ng lint",
    "lint:lib": "ng lint pip-webui2-controls",
    "e2e": "ng e2e",
    "package": "npm run build:lib && cd dist/pip-webui2-controls && npm pack",
    "deploy": "ng build --prod --base-href https://pip-webui2.github.io/pip-webui2-controls/ && ngh --dir dist/pip-webui2-controls-app"
  },
  "private": false,
